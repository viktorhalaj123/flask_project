{% extends "_main.jinja"%}

{% block title %} Articles Page {% endblock%}

{% block main %}
    {% if articles %}
        <ul>
      {% for id, article in articles %}
        <li>
            <h2><a href="{{ url_for("view_article", art_id=id)}}"> {{ article.title }} </a></h2>
            <p> {{ article.content }} </p>
        </li>
      {% endfor %}
        </ul>
    {% else %}
      <p> No Articles </p>
    {% endif %}
{% endblock %}
